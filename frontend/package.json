{
  "name": "llm-group-chat",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "con": "concurrently \"npm run con:client\" \"npm run con:server\"",
